{
    "name": "Youtube Disliked Extension",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Add new section 'Disliked' to Youtube to show a list of all videos that you disliked",
    "homepage_url": "https://duo.com/labs",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_title": "Youtube Disliked",
        "default_popup": "menu.html"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "https://*/*"
    ],
    "content_security_policy": "default-src 'self';",
    "content_scripts": [
        {
            "matches": [
                "https://*.youtube.com/*"
            ],
            "js": [
                "js/content.js"
            ]
        }
    ]
}