{
    "name": "Youtube Disliked Extension",
    "version": "",
    "manifest_version": 2,
    "description": "",
    "homepage_url": "https://github.com/father-gregor",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "browser_action": {
      "default_title": "Youtube Disliked",
      "default_popup": "menu.html"
    },
    "default_locale": "en",
    "background": {
      "scripts": [
        "js/background.js"
      ],
      "persistent": false
    },
    "permissions": [
      "https://*/*"
    ],
    "content_security_policy": "default-src 'self';",
    "content_scripts": [
      {
        "matches": ["https://*.youtube.com/*"],
        "js": ["js/content.js"]
      }
    ]
  }
  
